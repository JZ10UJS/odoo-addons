<openerp>
    <data>
        <!--删除安装crm自带的crm.team   这里是个巨坑-->
        <delete model="crm.stage" search="[('name','not in', ['Prospect','Qualification','Proposal','Agreement','Closed'])]"/>
        <!--<delete model="crm.stage" search="[('id', '=', ref('crm.stage_lead1'))]"/>-->
        <!--<delete model="crm.stage" search="[('id', '=', ref('crm.stage_lead2'))]"/>-->
        <!--<delete model="crm.stage" search="[('id', '=', ref('crm.stage_lead3'))]"/>-->
        <!--<delete model="crm.stage" search="[('id', '=', ref('crm.stage_lead4'))]"/>-->
        <!--<delete model="crm.stage" search="[('id', '=', ref('crm.stage_lead5'))]"/>-->
        <record id="yycrm_stage_1" model="crm.stage">
            <field name="name">Prospect</field>
            <field name="type" eval="'opportunity'"/>
            <field name="fold" eval="False"/>
            <field name="on_change" eval="True"/>
            <field name="probability" eval="25"/>
        </record>
        <record id="yycrm_stage_2" model="crm.stage">
            <field name="name">Qualification</field>
            <field name="type" eval="'opportunity'"/>
            <field name="fold" eval="False"/>
            <field name="on_change" eval="True"/>
            <field name="probability" eval="50"/>
        </record>
        <record id="yycrm_stage_3" model="crm.stage">
            <field name="name">Proposal</field>
            <field name="type" eval="'opportunity'"/>
            <field name="fold" eval="False"/>
            <field name="on_change" eval="True"/>
            <field name="probability" eval="75"/>
        </record>
        <record id="yycrm_stage_4" model="crm.stage">
            <field name="name">Agreement</field>
            <field name="type" eval="'opportunity'"/>
            <field name="fold" eval="False"/>
            <field name="on_change" eval="True"/>
            <field name="probability" eval="99"/>
        </record>
        <record id="yycrm_stage_5" model="crm.stage">
            <field name="name">Closed</field>
            <field name="type" eval="'opportunity'"/>
            <field name="fold" eval="True"/>
            <field name="on_change" eval="True"/>
            <field name="probability" eval="100"/>
        </record>

        <!--删除crm自建的Direct Sales-->
        <delete model="crm.team" search="[('name','ilike', 'Sales')]"/>
        <!--<delete model="crm.team" search="[('id', '=', ref('sales_team.team_sales_department'))]"/>-->
        <!--<delete model="crm.team" search="[('id', '=', ref('sales_team.crm_team_1'))]"/>-->
        <record id="yycrm_sale_team_1" model="crm.team">
            <field name="name">华东区</field>
            <field name="code" eval="'001'"/>
            <field name="alias_name" eval="'East China'"/>
            <field name="use_leads" eval="True"/>
            <field name="use_quotations" eval="True"/>
            <field name="stage_ids" eval="[(6, 0, [ref('yycrm_stage_1'),
                                                   ref('yycrm_stage_2'),
                                                   ref('yycrm_stage_3'),
                                                   ref('yycrm_stage_4'),
                                                   ref('yycrm_stage_5')])]"/>
        </record>
        <record id="yycrm_sale_team_2" model="crm.team">
            <field name="name">华北区</field>
            <field name="code" eval="'002'"/>
            <field name="alias_name" eval="'North China'"/>
            <field name="use_leads" eval="True"/>
            <field name="use_quotations" eval="True"/>
            <field name="stage_ids" eval="[(6, 0, [ref('yycrm_stage_1'),
                                                   ref('yycrm_stage_2'),
                                                   ref('yycrm_stage_3'),
                                                   ref('yycrm_stage_4'),
                                                   ref('yycrm_stage_5')])]"/>
        </record>
        <record id="yycrm_sale_team_3" model="crm.team">
            <field name="name">华中区</field>
            <field name="code" eval="'003'"/>
            <field name="alias_name" eval="'Central China'"/>
            <field name="use_leads" eval="True"/>
            <field name="use_quotations" eval="True"/>
            <field name="stage_ids" eval="[(6, 0, [ref('yycrm_stage_1'),
                                                   ref('yycrm_stage_2'),
                                                   ref('yycrm_stage_3'),
                                                   ref('yycrm_stage_4'),
                                                   ref('yycrm_stage_5')])]"/>
        </record>
        <record id="yycrm_sale_team_4" model="crm.team">
            <field name="name">华南区</field>
            <field name="code" eval="'004'"/>
            <field name="alias_name" eval="'South China'"/>
            <field name="use_leads" eval="True"/>
            <field name="use_quotations" eval="True"/>
            <field name="stage_ids" eval="[(6, 0, [ref('yycrm_stage_1'),
                                                   ref('yycrm_stage_2'),
                                                   ref('yycrm_stage_3'),
                                                   ref('yycrm_stage_4'),
                                                   ref('yycrm_stage_5')])]"/>
        </record>
        <record id="yycrm_sale_team_5" model="crm.team">
            <field name="name">华西区</field>
            <field name="code" eval="'005'"/>
            <field name="alias_name" eval="'West China'"/>
            <field name="use_leads" eval="True"/>
            <field name="use_quotations" eval="True"/>
            <field name="stage_ids" eval="[(6, 0, [ref('yycrm_stage_1'),
                                                   ref('yycrm_stage_2'),
                                                   ref('yycrm_stage_3'),
                                                   ref('yycrm_stage_4'),
                                                   ref('yycrm_stage_5')])]"/>
        </record>

        <record id="yycrm_sale_team_6" model="crm.team">
            <field name="name">其它</field>
            <field name="code" eval="'006'"/>
            <field name="alias_name" eval="'Other'"/>
            <field name="use_leads" eval="True"/>
            <field name="use_quotations" eval="True"/>
            <field name="stage_ids" eval="[(6, 0, [ref('yycrm_stage_1'),
                                                   ref('yycrm_stage_2'),
                                                   ref('yycrm_stage_3'),
                                                   ref('yycrm_stage_4'),
                                                   ref('yycrm_stage_5')])]"/>
        </record>

    </data>
</openerp>